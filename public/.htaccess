# 重定向根路径到PHP版本
RewriteEngine On

# 如果访问根路径，重定向到PHP版本
RewriteCond %{REQUEST_URI} ^/$
RewriteRule ^$ /original-home.php [L,R=302]

# 如果访问index.php，重定向到PHP版本
RewriteCond %{REQUEST_URI} ^/index\.php$
RewriteRule ^index\.php$ /original-home.php [L,R=302]

# 确保PHP文件可以正常访问
<Files "*.php">
    Require all granted
</Files>

# 设置默认首页
DirectoryIndex original-home.php index.php index.html